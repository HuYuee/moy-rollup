{
  "name": "moy-rollup",
  "version": "1.0.0",
  "description": "使用rollup对moy框架进行打包，和webpack打包进行对比",
  "main": "index.js",
  "scripts": {
    "dev": "rollup -c --environment TYPE:dev",
    "build": "rollup -c --environment TYPE:build",
    "upgrade": "npm uninstall tinper-sparrow compox compox-util tinper-neoui neoui-kero-mixin kero kero-fetch tinper-neoui-grid tinper-neoui-tree tinper-neoui-polyfill neoui-kero && npm install tinper-sparrow compox compox-util tinper-neoui neoui-kero-mixin kero kero-fetch tinper-neoui-grid tinper-neoui-tree tinper-neoui-polyfill neoui-kero --save",
    "product": "npm run build && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HuYuee/moy-rollup.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HuYuee/moy-rollup/issues"
  },
  "dependencies": {
    "compox": "^3.2.0",
    "compox-util": "^3.2.0",
    "kero": "^3.2.2",
    "kero-fetch": "^3.2.1",
    "neoui-kero": "^3.2.1",
    "neoui-kero-mixin": "^3.2.0",
    "tinper-neoui": "^3.2.1",
    "tinper-neoui-grid": "^3.2.1",
    "tinper-neoui-polyfill": "^3.2.0",
    "tinper-neoui-tree": "^3.2.1",
    "tinper-sparrow": "^3.2.1"
  },
  "homepage": "https://github.com/HuYuee/moy-rollup#readme",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-multidest": "^1.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-serve": "^0.3.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2"
  }
}
